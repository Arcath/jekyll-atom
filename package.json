{
  "name": "jekyll",
  "main": "./lib/main",
  "version": "2.1.0",
  "description": "A collection of snippets and tools for Jekyll",
  "activationCommands": {
    "atom-workspace": [
      "jekyll:open-layout",
      "jekyll:new-post",
      "jekyll:open-config",
      "jekyll:open-include",
      "jekyll:open-data",
      "jekyll:toggle-server",
      "jekyll:toggle-auto-build",
      "jekyll:new-post",
      "jekyll:build-site",
      "jekyll:publish-draft"
    ]
  },
  "repository": "https://github.com/arcath/jekyll-atom",
  "license": "MIT",
  "engines": {
    "atom": "*"
  },
  "dependencies": {
    "etch": "^0.12.2",
    "fs-plus": "^3.0.0",
    "js-yaml": "^3.8.3",
    "latinize": "^0.4.0",
    "static-server": "^2.0.4",
    "temp": "~0.8.3"
  },
  "keywords": [
    "jekyll",
    "snippets",
    "server",
    "build"
  ]
